{
	"name": "mvoc",
	"private": true,
	"scripts": {
		"test": "npm run test --workspaces",
		"mvoc:test": "npm run test --workspace mvoc",
		"mvoc:dev": "npm run dev --workspace mvoc",
		"mvoc:build:development": "ENVIRONMENT=development npm run build --workspace mvoc",
		"mvoc:build:staging": "ENVIRONMENT=staging npm run build --workspace mvoc",
		"mvoc:build:production": "ENVIRONMENT=production npm run build --workspace mvoc",
		"mvoc:preview:development": "ENVIRONMENT=development npm run preview --workspace mvoc",
		"mvoc:preview:staging": "ENVIRONMENT=staging npm run preview --workspace mvoc",
		"mvoc:preview:production": "ENVIRONMENT=production npm run preview --workspace mvoc",
		"mvoc:check-changed-documents:test": "ENVIRONMENT=development npm run check-changed-documents --workspace mvoc",
		"mvoc:check-changed-documents:staging": "ENVIRONMENT=staging npm run check-changed-documents --workspace mvoc",
		"mvoc:check-changed-documents:production": "ENVIRONMENT=production npm run check-changed-documents --workspace mvoc",
		"ui:test": "npm run test --workspace @mvoc/ui",
		"ui:dev": "npm run dev --workspace @mvoc/ui",
		"cms:dev": "npm run dev --workspace cms",
		"cms:deploy": "npm run deploy --workspace cms",
		"cms:build:development": "npm run build-development --workspace cms",
		"cms:build:staging": "npm run build-staging --workspace cms",
		"cms:build:main": "npm run build-main --workspace cms",
		"pza:dev": "npm run dev --workspace pza",
		"pza:build": "npm run build --workspace pza",
		"pza:preview": "npm run preview --workspace pza",
		"pza:test": "npm run test --workspace pza",
		"cms-pza:dev": "npm run dev --workspace cms-pza",
		"cms-pza:deploy": "npm run deploy --workspace cms-pza",
		"format": "npm run format:prettier",
		"format:prettier": "prettier --write ."
	},
	"workspaces": [
		"./packages/*"
	],
	"dependencies": {
		"@mvoc/ui": "^1.0.0",
		"typescript": "^5.3.3"
	}
}
