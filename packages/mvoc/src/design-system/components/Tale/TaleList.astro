---
import { TaleCollectionProps } from 'src/utilities/api/queries/translated';
import { Tale } from '.';
import { Stack } from '../Layout/Stack';

type Props = TaleCollectionProps;

const { taleCollection } = Astro.props;
---

<Stack class="c-tale" gap="huge">
	{taleCollection.map((tale) => <Tale {...tale} />)}
</Stack>
